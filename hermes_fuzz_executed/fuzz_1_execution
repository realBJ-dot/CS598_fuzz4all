'use strict';


class A {
  foo() {
    return 42;
  }
}

class B {
  bar() {
    return 42;
  }
}

class C extends B {
  constructor() {
    super();
    foo = this.foo;
    baz = 0;
  }
}

class D extends B {
  baz() {
    const {foo} = this;
  }
}
const D2 = D;

let a = new A();
let b = new B();
let c = new C();
let d = new D();
let d2 = new D2();
let x2 = 1;
let y = 1;

[1, '2'].forEach(number => {
  x2 = 2;
  y;
  return `Hello world! ${number}`;
});

(async () => {
  return 'awaited';
})();

async function test() {
  console.log(await 'awaited');
  console.log(y);
  console.log('x2', x2);
  console.log(a);
  console.log(c);
}

test();

class A2 {
  static b = 2;
  constructor(_a) {
    var foo = _a.foo, bar = _a.bar;
    var _b;
    baz = 1;
    return Object.assign(this, (_b = {}, _b[foo] = bar, _b));
  }
}

class B2 extends A2 {
  constructor() {
    super({foo: 'boo', bar: 'buzz'});
  }
}
console.log(new B2().b);


