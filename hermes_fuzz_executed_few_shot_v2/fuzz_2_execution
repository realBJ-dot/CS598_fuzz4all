'use strict';


let obj = {
    greetMe: (name, callback) => {
         
        setTimeout(() => {
             
            callback('Hello,'+ name);
        }, 1200);
    }
};
obj
  .greetMe('Alice')
  .then((res) => {
        console.log(res);
    })
  .catch((e) => {
        console.log(e);
    });

function greet(name, callback) {
     
    setTimeout(() => {
         
        callback('Hello,'+ name);
    }, 1200);
}

async function test() {
    console.log(await greet('Alice'));
}

setInterval(test, 1000);

 
 
 

 
function test() {
    console.log('Hi from a different isolate');
}
```

