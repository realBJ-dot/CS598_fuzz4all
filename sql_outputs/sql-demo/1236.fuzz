CREATE DATABASE IF NOT EXISTS sampleDB; USE sampleDB;
CREATE TABLE IF NOT EXISTS users (
userId CHAR(5) NOT NULL,
firstName VARCHAR(255),
lastName VARCHAR(255),
role CHAR(1) DEFAULT 'a' NOT NULL,
PRIMARY KEY (userId)
);


