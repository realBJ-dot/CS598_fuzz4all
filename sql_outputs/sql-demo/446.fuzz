CREATE DATABASE IF NOT EXISTS sampleDB; USE sampleDB;
CREATE TABLE mytable (b varchar(40) PRIMARY KEY, c int, d text, e date);
DROP TABLE IF EXISTS mytable;
-- Drop all tables.
DROP  DATABASE IF EXISTS sampleDB;

