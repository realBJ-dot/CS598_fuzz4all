CREATE DATABASE IF NOT EXISTS sampleDB; USE sampleDB;
CREATE TABLE Department (ID INT PRIMARY KEY, DEPARTMENT_NAME VARCHAR(50), DEPARTMENT_LOC VARCHAR(50));

DROP TABLE Student; DROP TABLE Department; use sampleDB;


